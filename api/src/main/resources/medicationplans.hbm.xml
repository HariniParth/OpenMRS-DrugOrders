<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.drugorders">
        
        <class name="medicationplans" table="standard_disease_plan">
            
            <id name="id" type="int" column="id" unsaved-value="0">
                    <generator class="native" />
            </id>
            <discriminator column="id" insert="false" />
            <property name="uuid" type="java.lang.String" column="uuid" length="38" unique="true" ></property>
            <property name="diseaseid" class="org.openmrs.Concept" column="diseaseid" ></property>
            <property name="drugid" class="org.openmrs.Concept" column="drugid" ></property>
            <property name="dose" type="java.lang.Double" length="10" column="dose" ></property>
            <property name="doseunits" class="org.openmrs.Concept" column="doseunits" ></property>
            <property name="quantity" type="java.lang.Double" length="10" column="quantity" ></property>
            <property name="quantityunits" class="org.openmrs.Concept" column="quantityunits" ></property>
            <property name="duration" column="duration" ></property>
            <property name="durationunits" class="org.openmrs.Concept" column="durationunits" ></property>
            <property name="route" class="org.openmrs.Concept" column="route" ></property>
            <property name="frequency" class="org.openmrs.OrderFrequency" column="frequency" ></property>
            
        </class>

</hibernate-mapping>
